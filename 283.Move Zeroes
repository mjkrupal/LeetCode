class Solution {
    public void moveZeroes(int[] nums) {
        int i=0;
        int position =0;
        while(i<nums.length){
            if(nums[i]!=0){
                nums[position]=nums[i];
                position++;
            }
            i++;
        }
        while(position<nums.length){
            nums[position]=0;
            position++;
        }
    }
}
